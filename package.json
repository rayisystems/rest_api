  "main": "dist",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w src --exec \"babel-node src --presets env,stage-2\"",
    "build": "babel src -s -D -d dist --presets env,stage-2",
    "start": "NODE_ENV=production pm2 start dist",
