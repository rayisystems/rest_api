    "express": "^4.16.2",
    "mongoose": "^4.13.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "nodemon": "^1.14.1"
  }
